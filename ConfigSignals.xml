		<Formulas>
			<Диапазон_макс/>
			<Диапазон_мин/>
			<range operation="-" operand1="Диапазон_макс" operand2="Диапазон_мин"/>
			<range1proc operation="/" operand1="range" operand2="100"/>
			<range3proc operation="*" operand1="range1proc" operand2="3"/>
			<range6proc operation="*" operand1="range1proc" operand2="6"/>
			<loLimF operation="-" operand1="Диапазон_мин" operand2="range3proc"/>
			<hiLimF operation="+" operand1="Диапазон_макс" operand2="range3proc"/>
			<loBrkF operation="-" operand1="Диапазон_мин" operand2="range6proc" />
			<hiBrkF operation="+" operand1="Диапазон_макс" operand2="range6proc"/>
		</Formulas>
		<GenData>
			<AI_DRV Type="REAL"/>
			<AI_Settings/>
			<AI_FromHMI/>
			<AI_ToHMI/>
			<AI_Internal/>
		</GenData>
		<GenCode>
			<!--File name="AI_CallAll.txt"-->
				<Function name="AI_init">
					<arguments>
						<arg><abonent/><text t="_AI_Settings."/><dbd t="TAG_NAME_PLC"/></arg>
						<arg><dbd t="Диапазон_мин"/></arg>
						<arg><dbd t="Диапазон_макс"/></arg>
						<arg><dbd t="АЦП"/></arg>
						<arg><dbd t="АЦПmax"/></arg>
