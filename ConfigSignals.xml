            <E nameColumnPos="limit" type="Number"/>
            <F nameColumnPos="delay" type="Number" default="3"/>
            <G nameColumnPos="тип" registr="up" default="У"  replace="true" repFrom1="U" repTo1="У" repFrom2="A" repTo2="А" repFrom3="P" repTo3="П" repFrom4="O" repTo4="О"/>
            <H nameColumnPos="defVal" type="Int" default="0"/>
            <I nameColumnPos="типПостфикс" switch="тип" var1="О" default1="огр." var2="А" default2="авар." var3="П" default3="предупр." default="упр." />
            <J nameColumnPos="директПостфикс" type="VarName" swith="direct" var1="больше" default1="верхняя" var2="меньше" default2="нижняя"/>
            <K nameColumnPos="typePostfix" switch="тип" var1="У" default1="_U"   var2="А" default2="_A"    var3="П" default3="_P"       var4="О" default4="_O"  />
            <L nameColumnPos="directPostfix"  type="VarName" switch="direct" var1="больше" default1="v"       var2="меньше" default2="n"     />
            <B nameColumnPos="Наименование" add1="типПостфикс" add2="директПостфикс"/>
            <F nameColumnPos="TAG_NAME_PLC" add1="TAG_NAME_AnPar" add2="typePostfix" add3="directPostfix"/>
        </EXEL>
