	<APS excelSheetName="APS" Comment="Аварино-предупредительная сигнализация">
		<EXEL>
			<B nameColumnPos="TAG_NAME_PLC" unical="true" type="VarName"/> <!-- Алгоритмическое имя аларма-->
			<C nameColumnPos="Наименование" /> <!--Русское название АПС-->
			<D nameColumnPos="Условие_срабатывания_АПС" /> <!--Логическое выражение -->
			<E nameColumnPos="Время_задержки" type="Number" default="0.0"/> <!--секунды-->
			<F nameColumnPos="Status" /> <!--строка из набора возм. значений: АО сс, АО бс, ВО сс, ВО бс, Пожар, ПС, АС -->
		</EXEL>
		<GenData>	<!-- генерация типов АПС -->
			<APS Type="BOOL"> 
				<globData name="APS"> 
					<localData int="_Algorithm.int" />
				</globData>
			</APS>
			<APS_Internal>
				<globData>
					<localData  name="APS_Internal" int="_Algorithm.int"/>
				</globData>
			</APS_Internal>	
		</GenData>
		<GenCode>
			<File src= "Call_APS"> <!-- target="_Algorithm"> генерация блока вызовов обработчиков -->
				<Block start="Начало сгенерированного кода APS_Processing" end="Конец сгенерированного кода">
					<Function name="APS_Processing">
						<arg><dbd t= "Условие_срабатывания_АПС"/></arg>
						<arg><text t=" "/><dbd t="Время_задержки"/></arg>
						<arg><text t=" "/><text t="unlock"/></arg>
						<arg><text t=" "/><abonent/><text t="_APS."/><dbd t="TAG_NAME_PLC"/></arg>
						<arg><text t=" "/><abonent/><text t="_APS_Internal."/><dbd t="TAG_NAME_PLC"/></arg>
						<arg><text t=" "/><abonent/><text t="_FirstOutIndex"/></arg>
						<arg><text t=" "/><text t="i"/></arg>
					</Function>	
				</Block>
			</File>
			<File src= "APS_Init"> <!-- target="_Algorithm"> генерация функции инициализации -->
				<Block start="Начало сгенерированного кода APS_Init" end="Конец сгенерированного кода">
					<String>
						<abonent/><text t="_APS_Internal."/><dbd t="TAG_NAME_PLC"/><text t=".target := Alarms."/>  
							<switch swt="Status" default="">
								<case val="АС" def="AS"/>
								<case val="ПС" def="PS"/>
								<case val="АО сс" def="AOs"/>
								<case val="АО бс" def="AOb"/>
								<case val="ВО сс" def="VOs"/>
								<case val="ВО бс" def="VOb"/>
								<case val="Пожар" def="Fire"/>
							</switch>
						 <text t="; //"/><dbd t="Наименование"/> 
					</String>						
				</Block>
			</File>
			
			<File src= "FirstOut.lua"> <!-- генерация lua таблицы имен алармов -->	
				<Block start="Начало таблицы" end="Конец таблицы" >
					<String>
						<IF cond="Status" val="ПС"> <text t=""/>
							<ELSE>
								<IF cond="Status" val="АС"> <text t=""/>
									<ELSE>
										<text t="'_APS."/><dbd t="TAG_NAME_PLC"/><text t="',  '"/><dbd t="Status"/><text t="',  - - "/><dbd t="Наименование"/> 
									</ELSE>
								</IF>
							</ELSE> 
						</IF> 
					</String>
				</Block>
			</File>	
		</GenCode>		
	</APS>
