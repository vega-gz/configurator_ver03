<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ConfigSignals>
	<AI excelSheetName="AI" Comment="Аналоговые входы">
        <EXEL>
            <F nameColumnPos="Наименование"/><!--Обязательный нода-->
