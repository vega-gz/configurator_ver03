<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!--Ноды "Наименование" и "TAG_NAME_PLC" должны присутствовать в каждой ноде "EXEL"-->
<ConfigSignals>
	<AI excelSheetName="AI" Comment="Аналоговые входы">
        <EXEL>
            <F nameColumnPos="Наименование"/><!--Обязательный нода-->
            <P nameColumnPos="TAG_NAME_PLC" unical="true" type="VarName" childTable="SetPoints" cildCol="TAG_NAME_AnPar"/>
            <G nameColumnPos="Диапазон_мин" type="Number" fRez="" default="0.0"/>
            <H nameColumnPos="Диапазон_макс" type="Number" fRez="" default="100.0"/>
            <J nameColumnPos="Тип_датчика" default="4-20 мА"/>
            <T nameColumnPos="Точность"  type="Int" default="2"/>
            <U nameColumnPos="АЦПmin" default="2.0"/>
            <V nameColumnPos="АЦПmax" default="10.0"/>
            <W nameColumnPos="loLim" type="Number" formula="loLimF"/>
            <X nameColumnPos="hiLim" type="Number" formula="hiLimF"/>
            <Y nameColumnPos="loBrk" type="Number" formula="loBrkF"/>
            <Z nameColumnPos="hiBrk" type="Number" formula="hiBrkF"/>
            <AA nameColumnPos="ROC" type="Number"   default="0.0"/>
            <AB nameColumnPos="recoveryTime" type="Number" default="1.0"/>
            <AC nameColumnPos="tau" type="Number"  default="1.0"/>
            <I nameColumnPos="Единица_измерения" default=""/>
            <K nameColumnPos="Акт_пас" default=""/>
            <L nameColumnPos="Адрес_1" default=""/>
            <M nameColumnPos="Адрес_2" default=""/>
            <N nameColumnPos="Адрес_3" default=""/>
            <Q nameColumnPos="Устройство" type="Int"/>
            <R nameColumnPos="Слот" type="Int"/>
            <S nameColumnPos="Канал" type="Int"/>
			<O nameColumnPos="Примечание" default=""/>
			<AD nameColumnPos="sonetModbus" default=""/>
        </EXEL>
		<Formulas>
			<Диапазон_макс/>
			<Диапазон_мин/>
			<range operation="-" operand1="Диапазон_макс" operand2="Диапазон_мин"/>
			<range1proc operation="/" operand1="range" operand2="100"/>
			<range3proc operation="*" operand1="range1proc" operand2="3"/>
			<range6proc operation="*" operand1="range1proc" operand2="6"/>
			<loLimF operation="-" operand1="Диапазон_мин" operand2="range3proc"/>
			<hiLimF operation="+" operand1="Диапазон_макс" operand2="range3proc"/>
			<loBrkF operation="-" operand1="Диапазон_мин" operand2="range6proc" />
			<hiBrkF operation="+" operand1="Диапазон_макс" operand2="range6proc"/>
		</Formulas>
		<GenData>
			<AI_DRV Type="REAL" globData1="AI_DRV">
			</AI_DRV>
			<AI_FromHMI globData1="AI_FromHMI">
				<ADD target="_ALG"/>
				<ADD target="AT_HMI"/>
			</AI_FromHMI>
			<AI_ToHMI   globData1="AI_ToHMI"> 
				<ADD target="Project.prj"/> 
			</AI_ToHMI>
			<AI_Settings>
			</AI_Settings>
			<AI_Internal>
			</AI_Internal>
		</GenData>
		<GenCode file="AI_CallAll">
			<Function name="AI_init">
				<arguments>
					<arg><abonent/><text t="_AI_Settings."/><dbd t="TAG_NAME_PLC"/></arg>
					<arg><dbd t="Диапазон_мин"/></arg>
					<arg><dbd t="Диапазон_макс"/></arg>
					<arg><dbd t="АЦПmin"/></arg>
					<arg><dbd t="АЦПmax"/></arg>
					<arg><dbd t="loLim"/></arg>
					<arg><dbd t="hiLim"/></arg>
					<arg><dbd t="loBrk"/></arg>
					<arg><dbd t="hiBrk"/></arg>
					<arg><dbd t="ROC"/></arg>
					<arg><dbd t="recoveryTime"/></arg>
					<arg><text t="repTime"/></arg>
					<arg><dbd t="tau"/></arg>
					<arg><npp/></arg>
				</arguments>
				<comment><dbd t="Наименование"/></comment>
			</Function>
			<Function name="AI_Processing">
				<arguments>
					<arg><abonent/><text t="_AI_DRV."/><dbd t="TAG_NAME_PLC"/></arg>
					<arg><abonent/><text t="_AI_Settings."/><dbd t="TAG_NAME_PLC"/></arg>
					<arg><abonent/><text t="_AI_FromHMI."/><dbd t="TAG_NAME_PLC"/></arg>
					<arg><abonent/><text t="_AI_ToHMI."/><dbd t="TAG_NAME_PLC"/></arg>
					<arg><abonent/><text t="_AI_Internal."/><dbd t="TAG_NAME_PLC"/></arg>
				</arguments>
				<comment><dbd t="Наименование"/></comment>
			</Function>
		</GenCode>
		<GenHW drvFile="_AI_DRV" globData="_AI_DRV"/>
	</AI>
	<SetPoints excelSheetName="ANB">
		<GenData>
			<T_ANB Type="BOOL"/>
			<T_GPA_SP_Settings Type="T_SP_Settings"/>
		</GenData>
        <EXEL>
            <C nameColumnPos="TAG_NAME_AnPar" type="VarName"/>
            <D nameColumnPos="direct" registr="down" replace="true" repFrom1="&gt;" repTo1="больше" repFrom2="true" repTo2="больше" repFrom3="&lt;" repTo3="меньше" repFrom4="false" repTo4="меньше"/><!---->
            <E nameColumnPos="limit" type="Number"/>
            <F nameColumnPos="delay" type="Number" default="3"/>
            <G nameColumnPos="тип" registr="up" default="У"  replace="true" repFrom1="U" repTo1="У" repFrom2="A" repTo2="А" repFrom3="P" repTo3="П" repFrom4="O" repTo4="О"/>
            <H nameColumnPos="defVal" type="Int" default="0"/>
            <I nameColumnPos="типПостфикс" switch="тип" var1="О" default1="огр." var2="А" default2="авар." var3="П" default3="предупр." default="упр." />
            <J nameColumnPos="директПостфикс" type="VarName" swith="direct" var1="больше" default1="верхняя" var2="меньше" default2="нижняя"/>
            <K nameColumnPos="typePostfix" switch="тип" var1="У" default1="_U"   var2="А" default2="_A"    var3="П" default3="_P"       var4="О" default4="_O"  />
            <L nameColumnPos="directPostfix"  type="VarName" switch="direct" var1="больше" default1="v"       var2="меньше" default2="n"     />
            <B nameColumnPos="Наименование" add1="типПостфикс" add2="директПостфикс"/>
            <F nameColumnPos="TAG_NAME_PLC" add1="TAG_NAME_AnPar" add2="typePostfix" add3="directPostfix"/>
        </EXEL>
	</SetPoints>
	<AnO excelSheetName="AO" Comment="Аналоговые выходы">
