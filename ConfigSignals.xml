            <AB nameColumnPos="АЦПmax" default="10.0"/>
            <AC nameColumnPos="loLim" type="Number" formula="loLimF"/>
            <AD nameColumnPos="hiLim" type="Number" formula="hiLimF"/>
            <AE nameColumnPos="loBrk" type="Number" formula="loBrkF"/>
            <AF nameColumnPos="hiBrk" type="Number" formula="hiBrkF"/>
