						<arg><dbd t="hiLim"/></arg>
						<arg><dbd t="loBrk"/></arg>
						<arg><dbd t="hiBrk"/></arg>
						<arg><dbd t="ROC"/></arg>
						<arg><dbd t="recoveryTime"/></arg>
						<arg><text t="repTime"/></arg>
						<arg><dbd t="tau"/></arg>
						<arg><npp/></arg>
					</Function>
				</Block>
