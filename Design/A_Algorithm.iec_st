            <Function UUID="35CD3CE44D6B4EC91CB7C0AF2904F6E6" Name="Graph_Processing" ResultTypeUUID="EC797BDD4541F500AD80A78F1F991834">
                <Variables>
                    <Variable UUID="99AE1348414E20F51355AFA4E83695E6" Name="graph" Type="T_Graph" TypeUUID="EE67E15142EC74278378C1A6B2ADA2E4" Usage="inout" />
                    <Variable UUID="2F7143C14717BEAC5460DC86749F56A9" Name="i" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="DD2C066B42F93597BF4CC78D4F6B31D3" Name="j" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                </Variables>
                <ST><![CDATA[for i := 0  to 32 do
	if graph.state = i then
		for j := 0  to 32 do
			if EXTRACT(graph.cond[i],j) then graph.state := j; end_if;
		end_for;
	end_if;
end_for;
]]></ST>
            </Function>
            <Function UUID="ED3998BB35478915F0456A93D24E78E8" Name="Mode_CallAll" ResultTypeUUID="EC797BDD4541F500AD80A78F1F991834">
                <Variables>
                    <Variable UUID="666C3ED6444B814FA96113BA02161B03" Name="Fire" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="49428CE14502B11CC54BE6B2BA92118F" Name="AOss" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="08FB86CA4B50E863FDFA6997CEE8B25E" Name="AObs" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="44A8D86C495C39C029324BBF9BE832C1" Name="VOss" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="8D8DCADB446C797A5C220080FFEB0C74" Name="VObs" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="3078962C4DEAB2F09DBF98896DE176EC" Name="NOss" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="CBE6A552464431A36DB58FA8443B7E80" Name="NObs" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="1F7B7B68451F2FF2183BD193805D3640" Name="Repair" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="6298C4AC452DE54E34F407A58F22C516" Name="HR" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="710B6A224E760229E7BD6293FF0536B5" Name="GR" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="610A90E54EC30D920B93DA8BE2806B15" Name="PZ" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="1A74CC7F4C87BBC05E7502985CB515B6" Name="PZM" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="51ADD77848E4DEEADDC781BBC48B439D" Name="HP" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="001F2F3845AB27D8958DE88D564CC498" Name="TP" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="D4800B974C8537316D6EAFAFA4BD4367" Name="TP2" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="A90375E94E1F61F4257EA6888C15FA6E" Name="KPK" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="00B9F23A428E141A437961BC4D836508" Name="APK" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="637154844F52A43234A13C8569CCDD7B" Name="APM" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="CD54372E40677B072A4A788248584A64" Name="K" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="9661E5794F173B32560799BEED0D36F9" Name="M" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="BAEAA1F54CFE07E3734FC789C8B9A299" Name="K_M" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="B72E53D8401D1CE4B2FFF3A7DDBF0A57" Name="M_K" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                </Variables>
                <ST><![CDATA[Fire  	:= 1;//Пожар
AOss  	:= 2;//АОсс
AObs  	:= 3;//АОбс
VOss  	:= 4;//ВОсс
VObs  	:= 5;//ВОбс
NOss  	:= 6;//НОсс
NObs  	:= 7;//НОбс
Repair  := 8;//Ремонт
HR  	:= 9;//Холодный резерв
GR  	:= 10;//Горячий резерв
PZ  	:= 11;//Проверка защит
PZM  	:= 12;//Проверка защит по маслу
HP  	:= 13;//Холодная прокрутка
TP  	:= 14;//Техн. прокрутка 1
TP2  	:= 15;//Техн. прокрутка 2
KPK  	:= 16;//КПК
APK  	:= 17;//АПК
APM  	:= 18;//АПМ
K  		:= 19;//Кольцо
M  		:= 20;//Магистраль
K_M  	:= 21;//Переход К М
M_K  	:= 22;//Переход М К
//------------------|Откуда|| Куда || Условия перехода -----------------------
//=== Пожар ========|======||======||=======================
PUTBIT(mde.graph.cond[Fire], Repair, 	drm.Fire.complete); 	// Из пожара в ремонт
//=== АОсс =========|======||======||=======================
PUTBIT(mde.graph.cond[AOss],  Fire, 		alg.Fire);				// Из АОсс в Пожар
PUTBIT(mde.graph.cond[AOss], Repair, 	drm.AOss.complete);		// Из АОсс в ремонт
//=== АОбс =========|======||======||=======================
PUTBIT(mde.graph.cond[AObs],  Fire, 		alg.Fire);				// Из АОбс в Пожар
PUTBIT(mde.graph.cond[AObs],  AOss, 		alg.AOss);				// Из АОбс в АОсс
PUTBIT(mde.graph.cond[AObs], Repair, 	drm.AObs.complete);		// Из АОбс в ремонт
//=== ВОсс =========|======||======||=======================
PUTBIT(mde.graph.cond[VOss],  Fire, 		alg.Fire);				// Из ВОсс в Пожар
PUTBIT(mde.graph.cond[VOss],  AOss, 		alg.AOss or alg.AObs);	// Из ВОсс в АОсс
PUTBIT(mde.graph.cond[VOss], Repair, 	drm.AObs.complete);		// Из ВОсс в ремонт
//=== ВОбс =========|======||======||=======================
PUTBIT(mde.graph.cond[VObs],  Fire, 		alg.Fire);				// Из ВОбс в Пожар
PUTBIT(mde.graph.cond[VObs],  AOss, 		alg.AOss);				// Из ВОбс в АОсс
PUTBIT(mde.graph.cond[VObs],  AObs, 		alg.AObs);				// Из ВОбс в АОсс
PUTBIT(mde.graph.cond[VObs],  VOss, 		alg.VOss);				// Из ВОбс в АОсс
PUTBIT(mde.graph.cond[VObs], Repair, 	drm.VObs.complete);		// Из ВОбс в ремонт
//=== НОсс =========|======||======||=======================
PUTBIT(mde.graph.cond[NOss],  Fire, 		alg.Fire);				// Из НОсс в Пожар
PUTBIT(mde.graph.cond[NOss],  AOss, 		alg.AOss or alg.AObs);	// Из НОсс в АОсс
PUTBIT(mde.graph.cond[NOss],  VOss, 		alg.VOss or alg.VObs );	// Из НОсс в АОсс
PUTBIT(mde.graph.cond[NOss], Repair, 	drm.NOss.complete and not alg.condHR and not alg.condGR);		// в ремонт
PUTBIT(mde.graph.cond[NOss],  HR, 		drm.NOss.complete and alg.condHR and not alg.condGR);			// в Холодный резерв
PUTBIT(mde.graph.cond[NOss],  GR, 		drm.NOss.complete and alg.condHR and alg.condGR);				// в Горячий резерв
//=== НОбс =========|======||======||=======================
PUTBIT(mde.graph.cond[NObs],  Fire, 		alg.Fire);				//в Пожар
PUTBIT(mde.graph.cond[NObs],  AOss, 		alg.AOss);	// в АОсс
