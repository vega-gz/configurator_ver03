        <Folder Name="ИМ" Comment="" Data="" UUID="256FD4E175404F99CD4A37BACE674CC7">
            <Function UUID="FE734D204A72971D36EB4B89631B06CB" Name="AM_AN_Processing" ResultTypeUUID="EC797BDD4541F500AD80A78F1F991834">
                <Variables>
                    <Variable UUID="3AFE7BC144EBA005E0C148BE81C120C4" Name="btn" Type="T_AnAM_Btn" TypeUUID="EB824DBB45D409040889D7B96D33A9CA" Usage="input" />
                    <Variable UUID="9255AC254C94055507943182F2B15286" Name="input" Type="T_AnAM_FromALG" TypeUUID="BB9385E64EB006274A5DDD9E2959A1CF" Usage="input" />
                    <Variable UUID="AC9ED7E345B386D36B58FD829B14D8DE" Name="set" Type="T_AnAM_Settings" TypeUUID="ED2E4A714A0F9A51D6443EB905AFDC78" Usage="input" />
                    <Variable UUID="5E07C2F54AD1854909A1FC8F563EBBB5" Name="internal" Type="T_AnAM_Internal" TypeUUID="362E073F4C91D3F3BF6443B04D1F1ABD" Usage="inout" />
                    <Variable UUID="CE6F3BC2407F32C0B0DB8CA48D8FB1ED" Name="hmi" Type="T_AnAM_ToHMI" TypeUUID="7F71797349DE56E6D8A3BB81C22F938F" Usage="inout" />
                    <Variable UUID="45C204F345082E9F60D2138A0EA905A9" Name="setAO" Type="T_AO_Settings" TypeUUID="4185BC3CA74E87CB5BCE3B9A33F614DF" Usage="input" />
                    <Variable UUID="ADDFB3B54B99F6D1103FAAA05FBD660B" Name="out" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="output" />
                </Variables>
                <ST><![CDATA[//обработка ДУ
hmi.modeManual:= input.prmManual and (btn.manualOn or hmi.modeManual) and not btn.manualOff;				//Режим ручного управления(BOOL)
//Определение неисправностей 
hmi.faultAlgPos := input.algPos < setAO.min or input.algPos > setAO.max;
hmi.faultManPos := btn.manualPos < setAO.min or btn.manualPos > setAO.max;
if hmi.modeManual then
	if not hmi.faultManPos then
		hmi.out := btn.manualPos;
	end_if;
else
	if not hmi.faultAlgPos then
		hmi.out := input.algPos;
		btn.manualPos := input.algPos;
	end_if;
end_if;
if input.plcPos > hmi.out + set.mismathLim or input.plcPos < hmi.out - set.mismathLim then
	internal.mismathTimer := internal.mismathTimer + cycle;
else
	internal.mismathTimer := 0.0;
end_if;
hmi.faultPos := internal.mismathTimer > set.mismathTimeLim;
hmi.fCommon := hmi.faultAlgPos or hmi.faultManPos or hmi.faultPos;//Обобщенная неисправность
out := hmi.out;]]></ST>
            </Function>
            <Function UUID="BCE28E164EF8DF775A7311900EC5C857" Name="AM_AN_init" ResultTypeUUID="EC797BDD4541F500AD80A78F1F991834">
