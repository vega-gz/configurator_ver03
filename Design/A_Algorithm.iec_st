                <Function UUID="8850B9CCDA4AABA5336C71BFA56CAD61" Name="AO_Processing" ResultTypeUUID="EC797BDD4541F500AD80A78F1F991834">
                    <Variables>
                        <Variable UUID="D4726F11CE41727D97F82EAE69A2F78D" Name="set" Type="T_AO_Settings" TypeUUID="4185BC3CA74E87CB5BCE3B9A33F614DF" Usage="input" />
                        <Variable UUID="EC657BECCB4C5CE6C1AB79B7F47292B2" Name="hmi" Type="T_AO_FromHMI" TypeUUID="28BBA2F94E53F5E7D050CBA8820F76E4" Usage="input" />
                        <Variable UUID="C6C2C120644DF1C6830948BDD4B1493F" Name="plc" Type="T_AO_ToHMI" TypeUUID="D225A65348D2770B341BAC9762DA1766" Usage="inout" />
                    </Variables>
                    <ST><![CDATA[plc.forsEnable := (plc.forsEnable or hmi.BTNforceOn) and plc.forsPermit and not hmi.BTNworkOn;
if plc.forsEnable then
	plc.PV := (hmi.manualTarget - set.min)/(set.max - set.min)*(set.maxDAC - set.minDAC)+set.minDAC;	
else
	plc.PV := (plc.algSet - set.min)/(set.max - set.min)*(set.maxDAC - set.minDAC)+set.minDAC;
	hmi.manualTarget := plc.algSet;
end_if;]]></ST>
                </Function>
