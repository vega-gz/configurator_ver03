                <Function UUID="1880AF7E994DE9942C11F19931227284" Name="DI_Processing" ResultTypeUUID="EC797BDD4541F500AD80A78F1F991834">
                    <Variables>
                        <Variable UUID="3319BE5EF74F26F7AF1505843BE08D07" Name="in" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                        <Variable UUID="502FCBB1F8451D2365B5648D5CA9E2E6" Name="externalFault" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                        <Variable UUID="7F9F63DCAF4FF7536C03D4A80493A683" Name="set" Type="T_DI_Settings" TypeUUID="94C521C642227325371AE7BCC363E527" Usage="input" />
                        <Variable UUID="31D488F165427AB6E900928E4552B9D7" Name="btn" Type="T_DI_FromHMI" TypeUUID="55B37D104E6E69A8BD7466B4968651A1" Usage="input" />
                        <Variable UUID="483D24D2EA4EF4A041A263855018E1CD" Name="out" Type="T_DI_ToHMI" TypeUUID="9F0EDD3143DC10ACF03CBA8C4A870F2D" Usage="inout" />
                        <Variable UUID="D9D16853A94AB561A8D803BBD28377D5" Name="my" Type="T_DI_Internal" TypeUUID="659357065D424EC1532DD5B5A8EDEF6B" Usage="inout" />
                        <Variable UUID="CF74E1091E4B667643419689BC2DD5AE" Name="tmp" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    </Variables>
                    <ST><![CDATA[//---------------------- Поиск ошибок задания настроек – несоответствие допустимым значениям (BOOL)-----------------------------------------
out.settingErr := set.repairTime = 0;
out.fault_common := out.settingErr or externalFault;//обобщённая неисправность. Образуется по «ИЛИ» от settingErr и неисправности цепи датчика
//---------------------------------------------------------------------------------------------------------------------------------------------
//------------------------------------------------------------------------------------------------------------------------------------------------
out.repair := (btn.BTNrepairOn or out.repair) and not(btn.BTNworkOn or btn.BTNmanualOn or out.CurrentTimeOfRepair > set.repairTime);//канал в ремонте (BOOL)
out.manual := (btn.BTNmanualOn or out.manual) and not(btn.BTNworkOn or btn.BTNrepairOn);//канал в дист.упр. (BOOL)
//------------------- Обработка ремонта ----------------------------------------------------------------------------------------------------------
if out.repair and not btn.BTNrepairProlong then
	my.timerRepair := my.timerRepair + cycle;//оставшееся время ремонта (INT)
else
	my.timerRepair := 0.0;
end_if;
tmp := set.repairTime - REAL_TO_INT(my.timerRepair);
if(tmp >= 0) then out.CurrentTimeOfRepair := tmp;
else out.CurrentTimeOfRepair := 0;
end_if;
out.repair_time_less_10_percent := out.CurrentTimeOfRepair < set.repairTime/10;//до конца ремонта осталось меньше 10% от заданного времени (BOOL)
if out.repair then return; end_if; //если выставлен ремонт - не обновляем значение параметра для алгоритма
//------------------------------------------------------------------------------------------------------------------------------------------------
//------------------- Обработка ДУ ----------------------------------------------------------------------------------------------------------
if out.manual then
	out.PV := btn.manualTarget;	//значение для САУ. Рассчитывается из  measure, Manual_Target с учётом неисправностей и фильтрации (REAL)
	return; 					//дальнейшие вычисления не нужны
else
	btn.manualTarget := out.PV;
end_if;
//------------------------------------------------------------------------------------------------------------------------------------------------
//------------------- Расчёт PV ----------------------------------------------------------------------------------------------------------
//значение для САУ
if set.del = 0.0 then
	out.PV := in;
else
    if not out.PV and in and my.timerAntiTremor < set.del then// or not out.PV and in then
        if my.timerAntiTremor < 0.0 then
            my.timerAntiTremor := 0.0;
        end_if;
        my.timerAntiTremor := my.timerAntiTremor + cycle;
    elsif out.PV and not in and my.timerAntiTremor > -set.del then// or not out.PV and in then
        if my.timerAntiTremor > 0.0 then
            my.timerAntiTremor := 0.0;
        end_if;
        my.timerAntiTremor := my.timerAntiTremor - cycle;
	else
		my.timerAntiTremor := 0.0;
    end_if;
    out.PV := in and my.timerAntiTremor >= set.del or out.PV and (in or my.timerAntiTremor > -set.del);
end_if;]]></ST>
                </Function>
