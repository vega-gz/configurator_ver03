            <Function UUID="667B9744471261C9AB2EB8B66C6E564F" Name="DRM_Call_CheckMode" ResultTypeUUID="EC797BDD4541F500AD80A78F1F991834">
                <Variables>
                    <Variable UUID="651F0BBC4C86F7D91A6A8A92061F7481" Name="End_drum" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                </Variables>
                <ST><![CDATA[if not init.DRM then
	drm.PZM.limTimer[2] := 10.0;
end_if;
//================================================================================================
//------------------------------------------------------------------------------------------------
//Drum: Проверка защит по маслу
drm.PZM.cond[1]:= drm.PZM.timer[1]>= drm.PZM.limTimer[1];
drm.PZM.cond[2]:= drm.PMS_N.complete and drm.PMS_D.complete and drm.PMS_R.complete;
drm.PZM.cond[3]:= drm.PMS_N.complete and drm.PMS_D.complete and drm.PMS_R.complete;
//--------------------------
DRM_Processing(drm.PZM, 4);
//--------------------------
dro.StartAlarm_PZM := drm.PZM.state = 1;
dro.PMS_N_PZM 	:= drm.PZM.state = 2;
dro.PMS_D_PZM 	:= drm.PZM.state = 2;
dro.PMS_R_PZM 	:= drm.PZM.state = 2;
dro.PNSD_OF_PZM := drm.PZM.state = 3;
dro.PNSN_OF_PZM := drm.PZM.state = 3;
dro.PNSR_OF_PZM := drm.PZM.state = 3;
//-------------------------
End_drum := true;
//------------------------------------------------------------------------------------------------]]></ST>
            </Function>
            <Function UUID="3EAC27784BCB1174707C23A8411652B5" Name="DRM_Call_StopMode" ResultTypeUUID="EC797BDD4541F500AD80A78F1F991834">
                <Variables>
                    <Variable UUID="7489E69C41DA8FCE6FCC469D2BDF2FB4" Name="End_drum" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                </Variables>
                <ST><![CDATA[//================================================================================================
//------------------------------------------------------------------------------------------------
//Drum: Аварийный останов по сигналу 'Пожар'
(*
drm.Fire.cond[1]:= GPA_DI_ToHMI.Kr5_ON.PV;
drm.Fire.cond[2]:= GPA_DI_ToHMI.Kr6_ON.PV;
drm.Fire.cond[3]:= GPA_DI_ToHMI.Kr1_OF.PV and GPA_DI_ToHMI.Kr2_OF.PV and GPA_DI_ToHMI.Kr4_OF.PV and 
				   GPA_DI_ToHMI.Kr5_ON.PV and GPA_DI_ToHMI.Kr6_ON.PV and GPA_DI_ToHMI.Kr6r_ON.PV and 
				   (GPA_DI_ToHMI.Kr12_OF.PV or GPA_DI_ToHMI.SK_OF.PV) and GPA_DI_ToHMI.Kr9_ON.PV and 
					frs.ST_notTurn and frs.GG_notTurn;*)
drm.Fire.cond[4]:= btn.Unlock;
//--------------------------
DRM_Processing(drm.Fire, 4);
//--------------------------
dro.Kr1_OF_Fire 	:= drm.Fire.state >= 1 and drm.Fire.state <= 3;
dro.Kr2_OF_Fire 	:= drm.Fire.state >= 1 and drm.Fire.state <= 3;
dro.Kr4_OF_Fire 	:= drm.Fire.state >= 1 and drm.Fire.state <= 3;
dro.Kr12_OF_Fire 	:= drm.Fire.state >= 1 and drm.Fire.state <= 3;
dro.SK_OF_Fire 		:= drm.Fire.state >= 1 and drm.Fire.state <= 3;
dro.Kr5_ON_Fire 	:= drm.Fire.state >= 1 and drm.Fire.state <= 3;
dro.Kr9_ON_Fire 	:= drm.Fire.state >= 1 and drm.Fire.state <= 3;
dro.Kr6_ON_Fire 	:= drm.Fire.state >= 2 and drm.Fire.state <= 3;
dro.Kr6r_ON_Fire 	:= drm.Fire.state = 3;
dro.StopToFR_Fire 	:= drm.Fire.state >= 1 and drm.Fire.state <= 3;
//-------------------------
End_drum := true;
//------------------------------------------------------------------------------------------------
//------------------------------------------------------------------------------------------------
//Drum: Аварийный останов со стравливанием газа
drm.AOss.cond[1]:= drm.StopD.complete;
drm.AOss.cond[2]:= drm.EmpN.complete;
drm.AOss.cond[3]:= btn.Unlock;
//--------------------------
DRM_Processing(drm.AOss, 3);
//--------------------------
dro.StopD_AOss 	:= drm.AOss.state = 1;
dro.EmpN_AOss 	:= drm.AOss.state = 2;
//---------------
End_drum := true;
//------------------------------------------------------------------------------------------------
//------------------------------------------------------------------------------------------------
//Drum: Аварийный останов без стравливания газа
drm.AObs.cond[1]:= drm.StopD.complete;
drm.AObs.cond[2]:= btn.Unlock;
//--------------------------
DRM_Processing(drm.AObs, 2);
//--------------------------
dro.StopD_AObs 	:= drm.AObs.state = 1;
//---------------
End_drum := true;
//------------------------------------------------------------------------------------------------
//Drum: Вынужденный останов со стравливанием газа
drm.VOss.cond[1]:= drm.M_K.complete;
drm.VOss.cond[2]:= drm.CoolD.complete;
drm.VOss.cond[3]:= drm.StopD.complete;
drm.VOss.cond[4]:= drm.EmpN.complete;
drm.VOss.cond[5]:= btn.Unlock;
//--------------------------
DRM_Processing(drm.VOss, 5);
//--------------------------
dro.M_K_VOss 	:= drm.VOss.state = 1;
dro.CoolD_VOss 	:= drm.VOss.state = 2;
dro.StopD_VOss 	:= drm.VOss.state = 3;
dro.EmpN_VOss 	:= drm.VOss.state = 4;
//---------------
End_drum := true;
//------------------------------------------------------------------------------------------------
//------------------------------------------------------------------------------------------------
//Drum: Вынужденный останов без стравливания газа
drm.VObs.cond[1]:= drm.M_K.complete;
drm.VObs.cond[2]:= drm.CoolD.complete;
drm.VObs.cond[3]:= drm.StopD.complete;
drm.VObs.cond[4]:= btn.Unlock;
//--------------------------
DRM_Processing(drm.VObs, 4);
//--------------------------
dro.M_K_VObs 	:= drm.VObs.state = 1;
dro.CoolD_VObs 	:= drm.VObs.state = 2;
dro.StopD_VObs 	:= drm.VObs.state = 3;
//---------------
End_drum := true;
//------------------------------------------------------------------------------------------------
//------------------------------------------------------------------------------------------------
//Drum: Нормальный останов со стравливанием газа
drm.NOss.cond[1]:= drm.M_K.complete;
drm.NOss.cond[2]:= drm.CoolD.complete;
drm.NOss.cond[3]:= drm.StopD.complete;
drm.NOss.cond[4]:= drm.EmpN.complete;
//--------------------------
DRM_Processing(drm.NOss, 4);
//--------------------------
dro.M_K_NOss 	:= drm.NOss.state = 1;
dro.CoolD_NOss 	:= drm.NOss.state = 2;
dro.StopD_NOss 	:= drm.NOss.state = 3;
dro.EmpN_NOss 	:= drm.NOss.state = 4;
//---------------
End_drum := true;
//------------------------------------------------------------------------------------------------
//------------------------------------------------------------------------------------------------
//Drum: Нормальный останов без стравливания газа
drm.NObs.cond[1]:= drm.M_K.complete;
drm.NObs.cond[2]:= drm.CoolD.complete;
drm.NObs.cond[3]:= drm.StopD.complete;
//--------------------------
DRM_Processing(drm.NObs, 3);
//--------------------------
dro.M_K_NObs 	:= drm.NObs.state = 1;
dro.CoolD_NObs 	:= drm.NObs.state = 2;
dro.StopD_NObs 	:= drm.NObs.state = 3;
//---------------
End_drum := true;
//------------------------------------------------------------------------------------------------
]]></ST>
            </Function>
            <Function UUID="B0EB641B42FAE22EC41835A5AD392238" Name="DRM_Call_SubMode" ResultTypeUUID="EC797BDD4541F500AD80A78F1F991834">
                <Variables>
                    <Variable UUID="7489E69C41DA8FCE6FCC469D2BDF2FB4" Name="End_drum" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                </Variables>
                <ST><![CDATA[if not init.DRM then
	drm.PMS.limTimer[2] := 5.0;
	drm.PMS.limTimer[3] := 20.0;
	drm.ZN.limTimer[2] := 10.0;
	drm.ZN.limTimer[4] := 60.0;
end_if;
//================================================================================================
//================================================================================================
//Drum: Запуск маслосистемы
drm.PMS.cond[1]:= NKU_DI_DRV.Pump_Start_m_GTD_ON and ANB.PmsmD_U1v;
drm.PMS.cond[2]:= drm.PMS.timer[2]>= drm.PMS.limTimer[2]; //5 sek
drm.PMS.cond[3]:= drm.PMS.timer[3]>= drm.PMS.limTimer[3] or NKU_DI_DRV.Pump_Start_m_GTD_ON and anb.dPm_g_U1v;
//------------------------------------------------------------------------------------------------
DRM_Processing(drm.PMS, 3);
//------------------------------------------------------------------------------------------------
dro.PNS_ON_PMS 	:= drm.PMS.state = 1;
dro.StabMsmN_PMS := drm.PMS.state = 2;
dro.PNU_ON_PMS 	:= drm.PMS.state = 3;
dro.PMS := drm.PMS.state >= 1 and drm.PMS.state <= 3;
//------------------------------------------------------------------------------------------------
End_drum := true;
//------------------------------------------------------------------------------------------------
//================================================================================================
//Drum: Заполнение нагнетателя
drm.ZN.cond[1]:= GPA_DI_ToHMI.Kr5_ON.PV and GPA_DI_ToHMI.Kr4_ON.PV;
drm.ZN.cond[2]:= drm.ZN.timer[2]>= drm.ZN.limTimer[2]; //10 sek
drm.ZN.cond[3]:= GPA_DI_ToHMI.Kr5_OF.PV and GPA_DI_ToHMI.Kr4_ON.PV;
drm.ZN.cond[4]:= drm.ZN.timer[4]>= drm.ZN.limTimer[4]; //60 sek
drm.ZN.cond[5]:= anb.PinN_Uv1 and anb.dPinNpo_dT_Un1; 
drm.ZN.cond[6]:= GPA_DI_ToHMI.Kr6_ON.PV and GPA_DI_ToHMI.Kr1_ON.PV;// and GPA_DI_ToHMI.Kr6r_OF.PV;
drm.ZN.cond[7]:= GPA_DI_ToHMI.Kr4_OF.PV;
//------------------------------------------------------------------------------------------------
DRM_Processing(drm.ZN, 7);
//------------------------------------------------------------------------------------------------
dro.Kr4_ON_ZN 	:= drm.ZN.state >= 1 and drm.ZN.state <= 5;
dro.Kr5_ON_ZN 	:= drm.ZN.state = 1 or drm.ZN.state = 2;
dro.Kr5_OF_ZN 	:= drm.ZN.state = 3;
dro.Kr1_ON_ZN 	:= drm.ZN.state = 6;
dro.Kr6_ON_ZN 	:= drm.ZN.state = 6;
dro.Kr6r_OF_ZN 	:= drm.ZN.state = 6;
dro.Kr4_OF_ZN 	:= drm.ZN.state = 7;
//------------------------------------------------------------------------------------------------
End_drum := true;
//------------------------------------------------------------------------------------------------
]]></ST>
            </Function>
            <Function UUID="6569A4404D018059A812D5B626BDA2B7" Name="DRM_Processing" ResultTypeUUID="EC797BDD4541F500AD80A78F1F991834">
                <Variables>
                    <Variable UUID="99AE1348414E20F51355AFA4E83695E6" Name="drum" Type="T_DRM" TypeUUID="2433DEE54E72840E369C8C97603D4040" Usage="inout" />
                    <Variable UUID="0C1C33EF4C10D3091713138A537FD2AA" Name="lastStep" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="input" />
                    <Variable UUID="2F7143C14717BEAC5460DC86749F56A9" Name="i" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                </Variables>
                <ST><![CDATA[if drum.enable then // Запуск маслосистемы
	//--------------------- Обнуление таймеров при старте графа --------------------------
	if drum.state = 0 then				
		for i := 0 to lastStep do					
			drum.timer[i] := 0.0;		
		end_for;
		drum.state := 1;				//автоматический переход на начало выполнения
	end_if;
	for i := 0  to lastStep do
		if drum.state = i then
			drum.timer[i] := drum.timer[i] + cycle;
			if drum.cond[i] then drum.state := i + 1; end_if;
		end_if;
	end_for;
else
	drum.state := 0;
end_if;
//------------- Формирование сообщения о завершении последовательности ----------------
drum.complete := drum.state = lastStep + 1;
]]></ST>
            </Function>
