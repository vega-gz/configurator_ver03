                        <Variable UUID="A049A8DD6744DDD60E7CB7BD7F8DB342" Name="del" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                        <Variable UUID="CA1DAB1B264D5EEE2F2C89B2B41C9796" Name="inv" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
