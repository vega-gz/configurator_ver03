    <Variables>
        <Variable UUID="2685EB6E61422E5F881839ACD239D471" Name="tmpREALtime" Type="LREAL" TypeUUID="65F1DDD44EDA9C0776BB16BBDFE36B1F" Usage="internal" />
    </Variables>
    <ST><![CDATA[//Определение длительности цикла
//cycle_time: 0-LOGIC, 1-RT,  2-AS
tmpREALtime := CLOCK();
cycle := LREAL_TO_REAL(tmpREALtime - tmpREALtime_prev);
tmpREALtime_prev := tmpREALtime;
//================================== Имитаторы ===================================
AM_All_imit();
//================================== ввод ==================================
//Обработка
AI_CallAll();
ANB_CallAll();
DI_CallAll();
//================================== Регуляторы ==================================
//Инициализация
//Обработка
//================================== ППУ ==================================
//Инициализация
//Обработка
//================================== Граф режимов ==================================
Mode_CallAll();
//================================== Барабаны ==================================
DRM_CallAll();
//================================== ИМ ==================================
//Резервированные
//Обычные
AM_CallAll();
//================================== Обмен по интерфейсам ==================================
//================================== вывод ==================================
AO_CallAll();
DO_CallAll();
]]></ST>
