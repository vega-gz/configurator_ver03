//========================== ИМы с аналоговым управлением ===============================
GPA_AnAM_FromALG.Vn1_MOR.plcPos := GPA_AI_ToHMI.Set_Vn1_MOR_KCU.PV;
AM_AN_Processing(GPA_AnAM_Btn.Vn1_MOR, GPA_AnAM_FromALG.Vn1_MOR, GPA_AnAM_Settings.Vn1_MOR, GPA_AnAM_Internal.Vn1_MOR, GPA_AnAM_ToHMI.Vn1_MOR, GPA_AO_Settings.Vn1_MOR, GPA_AO_ToHMI.Vn1_MOR.algSet);
]]></ST>
            </Function>
            <Function UUID="698D243E9544264A9EAF61BF7AED03FB" Name="AM_NKU_Processing" ResultTypeUUID="EC797BDD4541F500AD80A78F1F991834">
                <Variables>
                    <Variable UUID="19E691A8CA47BD20D45EF4AF022C4356" Name="setting" Type="T_AM_Setting_NKU" TypeUUID="7C637E8BBA4438EBAABF0BA4175DADD9" Usage="input" />
                    <Variable UUID="585F7900F542300CD71FF78C3974DDA5" Name="btn" Type="T_AM_Btn_NKU" TypeUUID="AF271DD4AC49825405BBD4B8444134C3" Usage="input" />
                    <Variable UUID="EF6B202CCF4874650A4C1DB170CB560A" Name="input" Type="T_AM_FromALG_NKU" TypeUUID="0E47956D904FE0E9F4EF2F922B64B8DB" Usage="input" />
                    <Variable UUID="DB693CDBE249FDE8D49454B9C249A24A" Name="my" Type="T_AM_Internal_NKU" TypeUUID="6B645B882C45D72359222082C04E4DCE" Usage="inout" />
                    <Variable UUID="F6FD5A5ABA4D45235824F493B095EB3E" Name="hmi" Type="T_AM_ToHMI_NKU" TypeUUID="F582C9A7F34B3863BB546DA1A8FD580E" Usage="inout" />
                    <Variable UUID="87C98D8A411D39CF1B34389057E97A5E" Name="wontOff" Comment="Условия отключения/закрытия" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                    <Variable UUID="6C74A0DF07492220574C5D92E9D05661" Name="wontOn" Comment="Условия включения/открытия" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                    <Variable UUID="17BBAB4E964E0B0848297192AC8B0083" Name="wontOnUPP" Comment="Условия включения через ППУ" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                </Variables>
                <ST><![CDATA[//обработка ДУ
hmi.modeManual:= input.prmManual and input.noPMU and (btn.manualOn or hmi.modeManual) and not btn.manualOff;				//Режим ручного управления(BOOL)
//========================================================================================================================================================================
//----------- Второй конечник и вторая команда оставлены для ИМ с командой на закрытие и/или с конечником закрытого положения 
IF setting.AMtype = AM_TYPE.am1x4nku	THEN
	//формирование внутренних установок ИМ
	wontOff := input.algOff and not hmi.modeManual or btn.off and hmi.modeManual or input.off and btn.stop or input.off_A;
	wontOn  := input.algOn  and not hmi.modeManual or btn.on;//  and hmi.modeManual or input.on  and btn.stop;
	my.off:= (wontOff or my.off) and not (wontOn  or not input.off and btn.stop);	//был algOff или btnOff(BOOL)
	my.on := (wontOn and input.ready  or my.on ) and not (wontOff or not input.on  and btn.stop);	//был algOn или btnOn(BOOL)
	//Таймер длительности команд
	if my.on and not (hmi.prmBtnOn and btn.on)     then my.timerOnCom := my.timerOnCom + cycle;//Таймер включения/открытия(REAL)
	else my.timerOnCom:= 0.0; end_if;
	if my.off and not (hmi.prmBtnOff and btn.off)  then my.timerOfCom := my.timerOfCom + cycle;//Таймер выключения/закрытия(REAL)
	else my.timerOfCom:= 0.0; end_if;
	//Формирование команд
	hmi.qOn := my.on;	//Выход - "включить" (открыть, форсировка)(BOOL)
	hmi.qOff:= not my.on;	//Выход - "отключить" (закрыть)(BOOL)
	//Формирование сигнализации
	my.wasOn  := my.on  and (input.on  or my.wasOn );
	my.wasOff := my.off and (input.off or my.wasOff);
	hmi.fSelfOn := my.wasOn  and not input.on;	//Самопроизвольное включение(BOOL)
	hmi.fSelfOff:= my.wasOff and not input.off;	//Самопроизвольное отключение(BOOL)
	hmi.fNotOn := my.timerOnCom > setting.delayNotMoveAlert and not (input.on  or hmi.fSelfOn );//Не включен(BOOL)
	hmi.fNotOff:= my.timerOfCom > setting.delayNotMoveAlert and not (input.off or hmi.fSelfOff);//Не отключен(BOOL)
	hmi.blockCommandNotReady := wontOn and not input.ready;
	hmi.fCommon:= hmi.fSelfOn or hmi.fSelfOff or hmi.fNotOn or hmi.fNotOff or //Обобщенная неисправность(BOOL)
				input.exFault or input.faultComOn or input.faultComOff;
//========================================================================================================================================================================
//-------------------------------------------- Для ИМ2х1 перед вызовом присваивать во второй конечник инверсию первого --------------------------------------------------- 
ELSIF setting.AMtype = AM_TYPE.am2x7upp THEN //
	//формирование внутренних установок ИМ
	wontOff:= input.algOff and not hmi.modeManual or btn.off and hmi.modeManual or not (input.on and input.onUPP) and btn.stop or input.off_A;
	//УПП запускается если (есть команда из Алг или нажата кнопка УПП) и ИМ не был включен штатно
	wontOnUPP := (input.algOn and not hmi.modeManual or btn.onUPP  and hmi.modeManual) and not my.wasOn;// or input.onUPP and btn.stop;
	//Условия прямого включения - если (есть команда из алгоритма или была кнопка на вкл через УПП) и (УПП сработало или сломано)
	wontOn :=   (input.algOn or wontOnUPP) and (input.onUPP or input.upp_A) and not hmi.modeManual or btn.on and hmi.modeManual or input.on and btn.stop; 
	my.off:=    (wontOff                   or my.off) and not (wontOn or wontOnUPP);		//был algOff или btnOff(BOOL)
	my.on :=    (wontOn and input.ready    or my.on ) and not wontOff;					//был algOn или btnOn(BOOL)
	my.onUPP := (wontOnUPP and input.readyUPP and input.ready or my.on ) and not (wontOff or my.wasOn);		//Включение через УПП активно пока не пришёл сигнал о нрмальном включении
	//Таймер длительности команд
	if my.on and not (hmi.prmBtnOn and btn.on) and not input.on  then my.timerOnCom := my.timerOnCom + cycle;//Таймер включения/открытия(REAL)
	else my.timerOnCom:= 0.0; end_if;
	if my.off and not (hmi.prmBtnOff and btn.off) and not input.off then my.timerOfCom := my.timerOfCom + cycle;//Таймер выключения/закрытия(REAL)
	else my.timerOfCom:= 0.0; end_if;
	//Таймер УПП
	if my.onUPP  then my.timerExtraOn  := my.timerExtraOn + cycle;		//Таймер используется для контроля включения УПП
	else my.timerExtraOn:= 0.0; end_if;
	if not my.onUPP  then my.timerExtraOff  := my.timerExtraOff + cycle;	//Таймер используется для контроля отключения УПП
	else my.timerExtraOff:= 0.0; end_if;
	//Блокировки выходных команд
	hmi.blockCommand := my.timerOnCom > setting.delayCommandTimeout or my.timerOfCom > setting.delayCommandTimeout ;	//Команда заблокирована таймером макс. длительности(BOOL)
	hmi.blockCommandNotReady := wontOn and not input.ready;		//Команда заблокирована из-за неготовности выходов или АС
	hmi.blockUPPnotReady  := wontOnUPP and not input.readyUPP;	//Команда на УПП заблокирована из-за неготовности УПП
	//Формирование команд
	hmi.qOn := my.on;	//Выход - "включить" (открыть, форсировка)(BOOL)
	hmi.qOnUPP := my.onUPP and not (my.timerExtraOn > setting.delayExtraOn or input.upp_A);	//Выход - "включить УПП" ограничен по времени
	//hmi.qOff:= my.off;	//Выход - "отключить" (закрыть)(BOOL) -- Не используется
	//Формирование сигнализации
	my.wasOn  := my.on  and (input.on  or my.wasOn );
	my.wasOff := my.off and (input.off or my.wasOff);
	hmi.fNotOn := my.timerOnCom > setting.delayCommandTimeout and not input.on;//Не включен(BOOL)
	hmi.fNotOff:= my.timerOfCom > setting.delayCommandTimeout and input.on;//Не отключен(BOOL)
	hmi.fNotOn_UPP := my.onUPP and (my.timerExtraOn > setting.delayExtraOn); 							//Не включился УПП
	hmi.fNotOff_UPP := not my.onUPP and input.onUPP and (my.timerExtraOff > setting.delayExtraOff);	//Не отключился УПП
	hmi.fCommon:= hmi.blockCommand or hmi.blockCommandNotReady or hmi.blockUPPnotReady or hmi.fNotOn_UPP or hmi.fNotOff_UPP or hmi.fNotOn or hmi.fNotOff or //Обобщенная неисправность(BOOL)
				input.exFault or input.faultComOn or input.faultComOff;
//========================================================================================================================================================================
ELSIF setting.AMtype = AM_TYPE.am2x8nku THEN //
	//формирование внутренних установок ИМ
	wontOff := input.algOff and not hmi.modeManual or btn.off and hmi.modeManual or input.off and btn.stop;
	wontOn  := input.algOn  and not hmi.modeManual or btn.on  and hmi.modeManual or input.on  and btn.stop;
	my.off:= (wontOff and input.ready or my.off) and not (wontOn  or not input.off and btn.stop or input.off_A or input.maxTorque);	//был algOff или btnOff(BOOL)
	my.on := (wontOn  and input.ready or my.on ) and not (wontOff or not input.on  and btn.stop or input.off_A or input.maxTorque);	//был algOn или btnOn(BOOL)
	//Таймер длительности команд
	if my.on and not  (hmi.prmBtnOn  and btn.on) and not input.on   then my.timerOnCom := my.timerOnCom + cycle;//Таймер включения/открытия(REAL)
	else my.timerOnCom:= 0.0; end_if;
	if my.off and not (hmi.prmBtnOff and btn.off) and not input.off then my.timerOfCom := my.timerOfCom + cycle;//Таймер выключения/закрытия(REAL)
	else my.timerOfCom:= 0.0; end_if;
	//Таймер поджимов
	if input.on  then my.timerExtraOn  := my.timerExtraOn + cycle;	//Таймер дожима на открытие(REAL)
	else my.timerExtraOn:= 0.0; end_if;
	if input.off then my.timerExtraOff := my.timerExtraOff + cycle;	//Таймер дожима на открытие(REAL)
	else my.timerExtraOff:= 0.0; end_if;
	// Определение количества одноимённых команд подряд
	if not hmi.qOn and my.prevComOn then my.cntRecommandOn := my.cntRecommandOn + 1; end_if;		//Счётчик поджимов(INT)
	if not my.on or btn.on or input.reset then my.cntRecommandOn := 0; end_if;
	my.prevComOn := hmi.qOn;
	if not hmi.qOff and my.prevComOff then my.cntRecommandOff := my.cntRecommandOff + 1; end_if;	//Счётчик поджимов(INT)
	if not my.off or btn.off or input.reset then my.cntRecommandOff := 0; end_if;
	my.prevComOff := hmi.qOff;
	//Блокировки выходных команд
	hmi.blockCommand := my.timerOnCom > setting.delayCommandTimeout or my.timerOfCom > setting.delayCommandTimeout ;	//Команда заблокирована таймером макс. длительности(BOOL)
	hmi.blockRecommand := my.cntRecommandOn > setting.qRecommand or my.cntRecommandOff > setting.qRecommand;			//Команда заблокирована счётчиком поджимов(BOOL)
	//Формирование команд
	hmi.qOn := my.on  and not (hmi.blockCommand or hmi.blockRecommand or my.timerExtraOn  > setting.delayExtraOn );	//Выход - "включить" (открыть, форсировка)(BOOL)
	hmi.qOff:= my.off and not (hmi.blockCommand or hmi.blockRecommand or my.timerExtraOff > setting.delayExtraOff);	//Выход - "отключить" (закрыть)(BOOL)
	hmi.prmBtnOn:= my.on  and (hmi.blockCommand or hmi.blockRecommand);	//Разрешение ручного включения в авто-режиме (для повтора команды)(BOOL)
	hmi.prmBtnOff:= my.off and (hmi.blockCommand or hmi.blockRecommand);	//Разрешение ручного отключения в авто-режиме (для повтора команды)(BOOL)
	//Формирование сигнализации
	my.wasOn  := my.on  and (input.on  or my.wasOn );
	my.wasOff := my.off and (input.off or my.wasOff);
	hmi.fSelfOn := my.wasOn  and not input.on;	//Самопроизвольное включение(BOOL)
	hmi.fSelfOff:= my.wasOff and not input.off;	//Самопроизвольное отключение(BOOL)
	hmi.fNotStart:= (hmi.qOn  and my.timerOnCom > setting.delayNotMoveAlert and (input.off or not input.opening)or										//Не стронулся(BOOL)
					hmi.qOff  and my.timerOfCom > setting.delayNotMoveAlert and (input.on  or not input.closing)) and not (hmi.fSelfOn or hmi.fSelfOff);
	hmi.fNotOn := my.timerOnCom > setting.delayCommandTimeout and not (input.on  or hmi.fSelfOn  or hmi.fNotStart);//Не включен(BOOL)
	hmi.fNotOff:= my.timerOfCom > setting.delayCommandTimeout and not (input.off or hmi.fSelfOff or hmi.fNotStart);//Не отключен(BOOL)
	hmi.fCommon:= hmi.blockCommand or hmi.blockRecommand or hmi.fSelfOn or hmi.fSelfOff or hmi.fNotStart or hmi.fNotOn or hmi.fNotOff or //Обобщенная неисправность(BOOL)
				input.exFault or input.faultComOn or input.faultComOff;
END_IF;
(*
//-------- для справки ---------
btn.on			//BOOL	Кнопка "включить/открыть" (СКАДА)
btn.off			//BOOL	Кнопка "отключить/закрыть" (СКАДА)
btn.stop		//BOOL	Кнопка Стоп
btn.manualOn	//BOOL	Кнопка включения режима Ручной (СКАДА)
btn.manualOff	//BOOL	Кнопка отключения режима Ручной (СКАДА)
btn.stop		//BOOL	Принять текущее положение, как правильное
input.on	//	BOOL	Конечник открыт/включен
input.off	//	BOOL	Конечник закрыт/выключен
input.algOn	//	BOOL	Команда из алгоритма на открыть/включить
input.algOff	//	BOOL	Команда из алгоритма на закрыть/выключить
input.prmManual	//	BOOL	Разрешение ручного управления
input.exFault	//	BOOL	Внешняя неисправность
input.reset	//	BOOL	Сброс счётчиков поджима и таймеров, блокирующих команды
input.faultComOn	//	BOOL	Неисправность команды открытия/включения
input.faultComOff	//	BOOL	Неисправность команды закрытия/выключения
setting.AMtype	//	INT	Тип ИМ
setting.delayCommandTimeout	//	REAL	Время, при превышении которого снимается выходная команда
setting.delayExtraOn	//	REAL	Время дожима на открытие
setting.delayExtraOff	//	REAL	Время дожима на закрытие
setting.delayNotMoveAlert	//	REAL	Задержка формирования нестрагивания
setting.qRecommand	//	INT	Разрешённое количество поджимов
*)]]></ST>
            </Function>
            <Function UUID="386A0C884B4F680F6B5385BB90E99634" Name="AM_Processing" ResultTypeUUID="EC797BDD4541F500AD80A78F1F991834">
                <Variables>
                    <Variable UUID="19E691A8CA47BD20D45EF4AF022C4356" Name="setting" Type="T_AM_Settings" TypeUUID="256A18694412558E2A55A4B7624DD172" Usage="input" />
                    <Variable UUID="585F7900F542300CD71FF78C3974DDA5" Name="btn" Type="T_AM_Btn" TypeUUID="EFC2F1704642389525D840801681051A" Usage="input" />
                    <Variable UUID="EF6B202CCF4874650A4C1DB170CB560A" Name="input" Type="T_AM_FromALG" TypeUUID="779CB6804A1EFE299C44259879EE559D" Usage="input" />
                    <Variable UUID="DB693CDBE249FDE8D49454B9C249A24A" Name="my" Type="T_AM_Internal" TypeUUID="32BA558A424E0C22E51AEAB8F6691BB0" Usage="inout" />
                    <Variable UUID="F6FD5A5ABA4D45235824F493B095EB3E" Name="hmi" Type="T_AM_ToHMI" TypeUUID="A6D7875840F9E702E7824F8046D18141" Usage="inout" />
                    <Variable UUID="73617E5344D2B9C0BCE50087D76A59FD" Name="wontOn" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                    <Variable UUID="87C98D8A411D39CF1B34389057E97A5E" Name="wontOff" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                </Variables>
                <ST><![CDATA[//обработка ДУ
hmi.modeManual:= input.prmManual and (btn.manualOn or hmi.modeManual) and not btn.manualOff;				//Режим ручного управления(BOOL)
//========================================================================================================================================================================
//CASE setting.AMtype OF
IF setting.AMtype = AM_TYPE.am1x0 THEN
	//формирование внутренних установок ИМ
	wontOff:= input.algOff and not hmi.modeManual or btn.off and hmi.modeManual;
	wontOn := input.algOn  and not hmi.modeManual or btn.on  and hmi.modeManual;
	my.off:= (wontOff or my.off) and not wontOn;	//был algOff или btnOff(BOOL)
	my.on := (wontOn  or my.on )  and not wontOff;	//был algOn или btnOn(BOOL)
	//Формирование команд
	hmi.qOn := my.on;		//Выход - "включить" (открыть, форсировка)(BOOL)
	hmi.qOff:= not my.on;	//Выход - "отключить" (закрыть)(BOOL)
	hmi.fCommon:= input.exFault or input.faultComOn or input.faultComOff;//Обобщенная неисправность(BOOL)
//========================================================================================================================================================================
//----------- Второй конечник и вторая команда оставлены для ИМ с командой на закрытие и/или с конечником закрытого положения 
ELSIF setting.AMtype = AM_TYPE.am1x1 or setting.AMtype = AM_TYPE.am1x2 or setting.AMtype = AM_TYPE.am2x2x3ware THEN
	//формирование внутренних установок ИМ
	wontOff:= input.algOff and not hmi.modeManual or btn.off and hmi.modeManual or not input.on  and btn.stop;
	wontOn := input.algOn  and not hmi.modeManual or btn.on  and hmi.modeManual or not input.off and btn.stop;
	my.off:= (wontOff or my.off) and not wontOn;	//был algOff или btnOff(BOOL)
	my.on := (wontOn  or my.on ) and not wontOff;	//был algOn или btnOn(BOOL)
	//Таймер длительности команд
	if my.on and not (hmi.prmBtnOn and btn.on)     then my.timerOnCom := my.timerOnCom + cycle;//Таймер включения/открытия(REAL)
	else my.timerOnCom:= 0.0; end_if;
	if my.off and not (hmi.prmBtnOff and btn.off)  then my.timerOfCom := my.timerOfCom + cycle;//Таймер выключения/закрытия(REAL)
	else my.timerOfCom:= 0.0; end_if;
	//Формирование команд
	hmi.qOn := my.on;	//Выход - "включить" (открыть, форсировка)(BOOL)
	hmi.qOff:= not my.on;	//Выход - "отключить" (закрыть)(BOOL)
	//Формирование сигнализации
	my.wasOn  := my.on  and (input.on  or my.wasOn );
	my.wasOff := my.off and (input.off or my.wasOff);
	hmi.fSelfOn := my.wasOn  and not input.on;	//Самопроизвольное включение(BOOL)
	hmi.fSelfOff:= my.wasOff and not input.off;	//Самопроизвольное отключение(BOOL)
	hmi.fNotOn := my.timerOnCom > setting.delayNotMoveAlert and not (input.on  or hmi.fSelfOn );//Не включен(BOOL)
	hmi.fNotOff:= my.timerOfCom > setting.delayNotMoveAlert and not (input.off or hmi.fSelfOff);//Не отключен(BOOL)
	hmi.fCommon:= hmi.fSelfOn or hmi.fSelfOff or hmi.fNotOn or hmi.fNotOff or //Обобщенная неисправность(BOOL)
				input.exFault or input.faultComOn or input.faultComOff;
//========================================================================================================================================================================
//-------------------------------------------- Для ИМ2х1 перед вызовом присваивать во второй конечник инверсию первого --------------------------------------------------- 
ELSIF setting.AMtype = AM_TYPE.am2x2 or setting.AMtype = AM_TYPE.am2x1 THEN //
	//формирование внутренних установок ИМ
	wontOff := input.algOff and not hmi.modeManual or btn.off and hmi.modeManual or input.off and btn.stop;
	wontOn := input.algOn  and not hmi.modeManual or btn.on  and hmi.modeManual or input.on  and btn.stop;
	my.off:= (wontOff or my.off) and not (wontOn  or not input.off and btn.stop);	//был algOff или btnOff(BOOL)
	my.on := (wontOn  or my.on ) and not (wontOff or not input.on  and btn.stop);	//был algOn или btnOn(BOOL)
	//Таймер длительности команд
	if my.on and not (hmi.prmBtnOn and btn.on) and not input.on  then my.timerOnCom := my.timerOnCom + cycle;//Таймер включения/открытия(REAL)
	else my.timerOnCom:= 0.0; end_if;
	if my.off and not (hmi.prmBtnOff and btn.off) and not input.off then my.timerOfCom := my.timerOfCom + cycle;//Таймер выключения/закрытия(REAL)
	else my.timerOfCom:= 0.0; end_if;
	//Таймер поджимов
	if input.on  then my.timerExtraOn  := my.timerExtraOn + cycle;	//Таймер дожима на открытие(REAL)
	else my.timerExtraOn:= 0.0; end_if;
	if input.off then my.timerExtraOff := my.timerExtraOff + cycle;	//Таймер дожима на открытие(REAL)
	else my.timerExtraOff:= 0.0; end_if;
	// Определение количества одноимённых команд подряд
	if not hmi.qOn and my.prevComOn then my.cntRecommandOn := my.cntRecommandOn + 1; end_if;		//Счётчик поджимов(INT)
	if not my.on or btn.on or input.reset then my.cntRecommandOn := 0; end_if;
	my.prevComOn := hmi.qOn;
	if not hmi.qOff and my.prevComOff then my.cntRecommandOff := my.cntRecommandOff + 1; end_if;	//Счётчик поджимов(INT)
	if not my.off or btn.off or input.reset then my.cntRecommandOff := 0; end_if;
	my.prevComOff := hmi.qOff;
	//Блокировки выходных команд
	hmi.blockCommand := my.timerOnCom > setting.delayCommandTimeout or my.timerOfCom > setting.delayCommandTimeout ;	//Команда заблокирована таймером макс. длительности(BOOL)
	hmi.blockRecommand := my.cntRecommandOn > setting.qRecommand or my.cntRecommandOff > setting.qRecommand;			//Команда заблокирована счётчиком поджимов(BOOL)
	//Формирование команд
	hmi.qOn := my.on  and not (hmi.blockCommand or hmi.blockRecommand or my.timerExtraOn  > setting.delayExtraOn );	//Выход - "включить" (открыть, форсировка)(BOOL)
	hmi.qOff:= my.off and not (hmi.blockCommand or hmi.blockRecommand or my.timerExtraOff > setting.delayExtraOff);	//Выход - "отключить" (закрыть)(BOOL)
	hmi.prmBtnOn:= my.on  and (hmi.blockCommand or hmi.blockRecommand);	//Разрешение ручного включения в авто-режиме (для повтора команды)(BOOL)
	hmi.prmBtnOff:= my.off and (hmi.blockCommand or hmi.blockRecommand);	//Разрешение ручного отключения в авто-режиме (для повтора команды)(BOOL)
	//Формирование сигнализации
	my.wasOn  := my.on  and (input.on  or my.wasOn );
	my.wasOff := my.off and (input.off or my.wasOff);
	hmi.fSelfOn := my.wasOn  and not input.on;	//Самопроизвольное включение(BOOL)
	hmi.fSelfOff:= my.wasOff and not input.off;	//Самопроизвольное отключение(BOOL)
	hmi.fNotStart:= (hmi.qOn  and my.timerOnCom > setting.delayNotMoveAlert and input.off or										//Не стронулся(BOOL)
					hmi.qOff and my.timerOfCom > setting.delayNotMoveAlert and input.on) and not (hmi.fSelfOn or hmi.fSelfOff);
	hmi.fNotOn := my.timerOnCom > setting.delayCommandTimeout and not (input.on  or hmi.fSelfOn  or hmi.fNotStart);//Не включен(BOOL)
	hmi.fNotOff:= my.timerOfCom > setting.delayCommandTimeout and not (input.off or hmi.fSelfOff or hmi.fNotStart);//Не отключен(BOOL)
	hmi.fCommon:= hmi.blockCommand or hmi.blockRecommand or hmi.fSelfOn or hmi.fSelfOff or hmi.fNotStart or hmi.fNotOn or hmi.fNotOff or //Обобщенная неисправность(BOOL)
				input.exFault or input.faultComOn or input.faultComOff;
//========================================================================================================================================================================
ELSIF setting.AMtype = AM_TYPE.am3x2 THEN //
	//формирование внутренних установок ИМ
	my.off:= input.algOff and not hmi.modeManual and (btn.off or my.off) and hmi.modeManual and not btn.stop;	//был algOff или btnOff(BOOL)
	my.on := input.algOn  and not hmi.modeManual and (btn.on  or my.on ) and hmi.modeManual and not btn.stop;	//был algOn или btnOn(BOOL)
	//Таймер длительности команд
	if my.on and not (hmi.prmBtnOn and btn.on)     then my.timerOnCom := my.timerOnCom + cycle;//Таймер включения/открытия(REAL)
	else my.timerOnCom:= 0.0; end_if;
	if my.off and not (hmi.prmBtnOff and btn.off)  then my.timerOfCom := my.timerOfCom + cycle;//Таймер выключения/закрытия(REAL)
	else my.timerOfCom:= 0.0; end_if;
	//Таймер поджимов
	if input.on  then my.timerExtraOn  := my.timerExtraOn + cycle;	//Таймер дожима на открытие(REAL)
	else my.timerExtraOn:= 0.0; end_if;
	if input.off then my.timerExtraOff := my.timerExtraOff + cycle;	//Таймер дожима на открытие(REAL)
	else my.timerExtraOff:= 0.0; end_if;
	// Определение количества одноимённых команд подряд
	if not hmi.qOn and my.prevComOn then my.cntRecommandOn := my.cntRecommandOn + 1; end_if;		//Счётчик поджимов(INT)
	if not my.on or btn.on or input.reset then my.cntRecommandOn := 0; end_if;
	my.prevComOn := hmi.qOn;
	if not hmi.qOff and my.prevComOff then my.cntRecommandOff := my.cntRecommandOff + 1; end_if;	//Счётчик поджимов(INT)
	if not my.off or btn.off or input.reset then my.cntRecommandOff := 0; end_if;
	my.prevComOff := hmi.qOff;
	//Блокировки выходных команд
	hmi.blockCommand := my.timerOnCom > setting.delayCommandTimeout or my.timerOfCom > setting.delayCommandTimeout ;	//Команда заблокирована таймером макс. длительности(BOOL)
	hmi.blockRecommand := my.cntRecommandOn > setting.qRecommand or my.cntRecommandOff > setting.qRecommand;			//Команда заблокирована счётчиком поджимов(BOOL)
	//Формирование команд
	hmi.qOn := my.on  and not (hmi.blockCommand or hmi.blockRecommand or my.timerExtraOn  > setting.delayExtraOn );	//Выход - "включить" (открыть, форсировка)(BOOL)
	hmi.qOff:= my.off and not (hmi.blockCommand or hmi.blockRecommand or my.timerExtraOff > setting.delayExtraOff);	//Выход - "отключить" (закрыть)(BOOL)
	hmi.qHoldOn:= not (my.on or my.off);//Выход - "удержание" (трехходовой клапан)(BOOL)
	hmi.prmBtnOn:= my.on  and (hmi.blockCommand or hmi.blockRecommand);	//Разрешение ручного включения в авто-режиме (для повтора команды)(BOOL)
	hmi.prmBtnOff:= my.off and (hmi.blockCommand or hmi.blockRecommand);	//Разрешение ручного отключения в авто-режиме (для повтора команды)(BOOL)
	//Формирование сигнализации
	my.wasOn  := (my.on  and input.on  or my.wasOn ) and not my.off;
	my.wasOff := (my.off and input.off or my.wasOff) and not my.on;
	hmi.fSelfOn := my.wasOn  and not input.on;//Самопроизвольное включение(BOOL)
	hmi.fSelfOff:= my.wasOff and not input.off;//Самопроизвольное отключение(BOOL)
	hmi.fNotStart:= (hmi.qOn and my.timerOnCom > setting.delayNotMoveAlert and input.off or										//Не стронулся(BOOL)
					hmi.qOff and my.timerOfCom > setting.delayNotMoveAlert and input.on) and not (hmi.fSelfOn or hmi.fSelfOff);
	hmi.fNotOn := my.timerOnCom > setting.delayCommandTimeout and not (input.on  or hmi.fSelfOn  or hmi.fNotStart);//Не включен(BOOL)
	hmi.fNotOff:= my.timerOfCom > setting.delayCommandTimeout and not (input.off or hmi.fSelfOff or hmi.fNotStart);//Не отключен(BOOL)
	hmi.fCommon:= hmi.blockCommand or hmi.blockRecommand or hmi.fSelfOn or hmi.fSelfOff or hmi.fNotStart or hmi.fNotOn or hmi.fNotOff or //Обобщенная неисправность(BOOL)
				input.exFault or input.faultComOn or input.faultComOff;
END_IF;
(*
//-------- для справки ---------
btn.on			//BOOL	Кнопка "включить/открыть" (СКАДА)
btn.off			//BOOL	Кнопка "отключить/закрыть" (СКАДА)
btn.stop		//BOOL	Кнопка Стоп
btn.manualOn	//BOOL	Кнопка включения режима Ручной (СКАДА)
btn.manualOff	//BOOL	Кнопка отключения режима Ручной (СКАДА)
btn.takePos		//BOOL	Принять текущее положение, как правильное
input.on	//	BOOL	Конечник открыт/включен
input.off	//	BOOL	Конечник закрыт/выключен
input.algOn	//	BOOL	Команда из алгоритма на открыть/включить
input.algOff	//	BOOL	Команда из алгоритма на закрыть/выключить
input.prmManual	//	BOOL	Разрешение ручного управления
input.exFault	//	BOOL	Внешняя неисправность
input.reset	//	BOOL	Сброс счётчиков поджима и таймеров, блокирующих команды
input.faultComOn	//	BOOL	Неисправность команды открытия/включения
input.faultComOff	//	BOOL	Неисправность команды закрытия/выключения
setting.AMtype	//	INT	Тип ИМ
setting.delayCommandTimeout	//	REAL	Время, при превышении которого снимается выходная команда
setting.delayExtraOn	//	REAL	Время дожима на открытие
setting.delayExtraOff	//	REAL	Время дожима на закрытие
setting.delayNotMoveAlert	//	REAL	Задержка формирования нестрагивания
setting.qRecommand	//	INT	Разрешённое количество поджимов
*)]]></ST>
            </Function>
