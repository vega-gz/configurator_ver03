			</disableVarDeclaration>
			<addVarDeclaration>
				<VarDeclaration InitialValue="''" Name="modePZ" Type="STRING" TypeUUID="38FDDE3B442D86554C56C884065F87B7" />
				<VarDeclaration InitialValue="''" Name="Reset_PZ" Type="STRING" TypeUUID="38FDDE3B442D86554C56C884065F87B7" UUID=""/>
				<VarDeclaration InitialValue="''" Name="Deblock" Type="STRING" TypeUUID="38FDDE3B442D86554C56C884065F87B7" UUID=""/>
				<VarDeclaration InitialValue="''" Name="ChngAb" Type="STRING" TypeUUID="38FDDE3B442D86554C56C884065F87B7" UUID=""/>
			</addVarDeclaration>
			<Disable>
				<hint/>
				<visiblePar/>
				<disableAlarm/>
				<Analog_Ph_N/>
				<PrefStr/>
				<TagID/>
			</Disable>
			<IF cond="Status" val="ПС">
				<BREAK/>
				<ELSE>
					<IF cond="Status" val="АС">
						<BREAK/>
					</IF>
				</ELSE>	
			</IF>				
			<additionalVar>
				<Delay tableCol="Время_задержки" Type="STRING"/> 
				<!--<NameAS tableCol="TAG_NAME_PLC" Type="STRING"/> -->
				<NameSigD Type="STRING"/>
				<IF cond="Источник" val="*.PV"> <!-- если заканчивается на .PV-->
					<NameSigA Type="STRING" tableCol="Источник"/>
					<NameUst Type="STRING" tableCol="TAG_NAME_PLC"/>
					<Edit>
						<NameSigD value="" Type="STRING" />
					</Edit>	
					<ELSE> 
						<Edit>
							<NameSigA value="" Type="STRING" />
						</Edit>	
						<IF cond="Источник" val="">
							<!--<NameSigD Type="STRING" text="_APS." tableCol="TAG_NAME_PLC"/>	 придумать как срастить в строку -->
							<Edit>
								<NameSigD text="_APS." tableCol="TAG_NAME_PLC" Type="STRING" /> 
							</Edit>	
								<ELSE>
									<Edit>
										<NameSigD tableCol="Источник" Type="STRING" /> 
									</Edit>										
								</ELSE> 
						</IF>
					</ELSE>
				</IF>
			</additionalVar>
		</GenHMI>			
	</APS>
