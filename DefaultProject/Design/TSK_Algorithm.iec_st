                    </Variables>
                    <ST><![CDATA[repTime := 3600;
if init = false then

//------------------------------- аргументы функции инициализации --------------------------------
//минимум шкалы канала, ед.изм. Не может быть >= max
//максимум шкалы канала, ед.изм. Не может быть <= min
//значение АЦП, соответствующее минимуму шкалы канала, б.р.
//значение АЦП, соотв. макс. шкалы, б.р. Не может быть равно minADC
//уровень зашкала вниз, ед.изм. Не может быть >= hiLim и < min
//уровень зашкала вверх, ед.изм. Не может быть <= loLim и > max
//уровень зашкала вниз, ед.изм. Не может быть >= loLim
//уровень обрыва вверх, ед.изм. Не может быть <= hiLim
//макс. допустимая скорость роста, ед.изм./сек. Если «0» - скорость роста не анализируется
//время восстановления после неисправности, сек. Не может быть меньше "0"
//максимальное время в ремонте, сек. Не может быть меньше или равно «0»
//тау фильтра, сек. Если «0» - фильтрация отсутствует
//номер по порядку	
//-------------------------------------------------------------------------------------------
//Начало сгенерированного кода AI_init
//Температура воздуха в ББ САУ ТСк ПХГ TТ-100
AI_init(TSK_AI_Settings.T_BB_TSK,-50.0,50.0,11773.0,59714.0,-53.0,53.0,-56.0,56.0,0.0,1.0,repTime,1.0,0);
//Трубное давление в скважине 1Т (НКТ 114мм) РТ-11
AI_init(TSK_AI_Settings.Pg_1T_t,0.0,25.0,11773.0,59714.0,-0.75,25.75,-1.5,26.5,0.0,1.0,repTime,1.0,1);
//Межколонное давление в скважине 1Т (между колоннами 114-178мм)  РТ-12
AI_init(TSK_AI_Settings.Pg_1T_mk1,0.0,25.0,11773.0,59714.0,-0.75,25.75,-1.5,26.5,0.0,1.0,repTime,1.0,2);
//Межколонное давление в скважине 1Т (между колоннами 178-245мм) РТ-13
AI_init(TSK_AI_Settings.Pg_1T_mk2,0.0,10.0,11773.0,59714.0,-0.3,10.3,-0.6,10.6,0.0,1.0,repTime,1.0,3);
//Межколонное давление в скважине 1Т (между колоннами 245-324мм) РТ-14
AI_init(TSK_AI_Settings.Pg_1T_mk3,0.0,10.0,11773.0,59714.0,-0.3,10.3,-0.6,10.6,0.0,1.0,repTime,1.0,4);
//Температура газа на скважине 1Т TТ-11
AI_init(TSK_AI_Settings.Tg_1T,-50.0,50.0,11773.0,59714.0,-53.0,53.0,-56.0,56.0,0.0,1.0,repTime,1.0,5);
