        <Variable UUID="4FYWCQGFVTTENAXW26GXL2TR7A" Name="tmpBTN_ArchUser" Type="BOOL" Usage="internal" />
    </Variables>
    <ST><![CDATA[//Определение длительности цикла
//cycle_time:
tim.tmpREALtime := CLOCK();
tim.cycle := LREAL_TO_REAL(tim.tmpREALtime - tim.tmpREALtime_prev);
tim.tmpREALtime_prev := tim.tmpREALtime;
//================================== Имитаторы ===================================
if ImitOn then Imit(); end_if;
//================================== ввод ==================================
//Обработка
TSK_AI_CallAll();
ANB_CallAll();
TSK_DI_CallAll();
//=== Драйверы смежных подсистем ===

//= здесь будут драйверы

//================================== АПС ==================================
APS_Init();
APS_CallAll(TSK_BTN.Deblock);
//================================== Регуляторы ==================================
//Инициализация
//Обработка
//================================== Логика =====================================
Logic();
//================================== ППУ ==================================
//Инициализация
//Обработка
//================================== Граф режимов ==================================
Mode_CallAll();
//================================== Барабаны ==================================
//DRM_CallAll();
//================================== Алгоритм ==================================
Alg_TSK();
//================================== ИМ ==================================
//Резервированные
//Обычные
IM_Call();
//================================== Тренды ==================================
Trends(); // передача данных в тренды
//= запись архивов
TSK_SS.ArchStop := TSK_Mode.AOTSK;//= Архив останова
//= включение/выключение Пользовательского архива
tmpBTN_ArchUser := TSK_BTN.ArchUser;
TSK_BTN.ArchUser := FALSE;
if tmpBTN_ArchUser then
  if TSK_SS.ArchUser Then
    TSK_SS.ArchUser := FALSE;
  else
    TSK_SS.ArchUser := TRUE;
  end_if;
end_if;
//================================== Обмен по интерфейсам ==================================
//================================== вывод ==================================
TSK_DO_CallAll();

//меандр
Pulse(true, T#1s, TSK_SS.CPU_HeartBit);
init:= true;//=]]></ST>
