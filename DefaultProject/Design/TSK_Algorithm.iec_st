                    <Variable UUID="NB5XWH7SF4PULE6RHMMEANS5QA" Name="Norma" Type="INT" Usage="internal" />
                    <Variable UUID="GMMSZZIWZ2QENHQ6ZGJYWZVZQM" Name="Fault" Type="INT" Usage="internal" />
                </Variables>
                <ST><![CDATA[//ТСк
Fault  	:= 0;//Нет режима
//
AO  	:= 1;//АО
Norma  	:= 2;//Норма

//===========================================================================================================================================================================================

//=== АО    ========|======||======||=======================
if not algTSK.AO 	then mdeTSK.graph.cond[AO] := PUTBIT(mdeTSK.graph.cond[AO],  Norma, true); end_if;		// Из АО в Норма
//=== Норма  ========|======||======||=======================
if algTSK.AO 		then mdeTSK.graph.cond[Norma] := PUTBIT(mdeTSK.graph.cond[Norma],AO, 	true); end_if;		// в АО
//===========================================================================================================================================================================================
//============================================================================
TSK_Mode.Num := Graph_Processing(mdeTSK.graph);
//============================================================================
//===========================================================================================================================================================================================

TSK_Mode.AOTSK 	:=  mdeTSK.graph.state = AO;	//АО
TSK_Mode.NormaTSK	:=  mdeTSK.graph.state = Norma;//Резерв

TSK_Mode.FaultTSK := not (TSK_Mode.NormaTSK or TSK_Mode.AOTSK);// Нет режима
]]></ST>
